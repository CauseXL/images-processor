    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "2.3.2",
